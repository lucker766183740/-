/* components/simple/simple.wxss */
.container{
  display: flex;
  flex-wrap: wrap;
}
.card-image{
  width: 240rpx;
  height:350rpx;
  padding: 10rpx;
  box-sizing: border-box;
  text-align: center;
}
.image_container{
  width: 220rpx;
  height: 270rpx;
}
.image_container  image{
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
}
.card-image>view{
  width: 100%;
  text-align: center;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.card-image text{
  font-size: 28rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}