/* components/likeAndcollection/likeAndcollection.wxss */
@import '/style/icon.wxss';
.container{
  width: 400rpx;
  height: 500rpx;
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50% , -50%);
  background-color: #fff;
  border-radius: 20rpx;
}
.container view{
  color: #333;
  text-align: center;
  line-height: 90rpx;
  font-size: 24rpx;
}
.title{
  border-bottom: 1rpx solid #ddd;
  font-size: 28rpx;
  font-weight: 550;
}
.btn{
  margin-top: 20rpx;
}
.btn text{
  background-color: red;
  color: #fff;
  padding: 10rpx 50rpx;
  border-radius: 50rpx;
}
.icon-zhangjieshu{
  color: #004aaa;
}
.icon-xinaixin{
  color: red;
}
.icon-wujiaoxing{
  color: orange;
}