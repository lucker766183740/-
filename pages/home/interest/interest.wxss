/* pages/home/interest/interest.wxss */
.interest{
  width: 750rpx;
  display: flex;
flex-wrap: wrap;
  padding: 35rpx;
  box-sizing: border-box;
}
.interest_title{
  width:150rpx;
  margin: 10rpx 0 ;
}
.interest_content{
  display: flex;
  flex-wrap: wrap;
  width: 525rpx;
}
.interest_content>text{
  display: inline-block;
  border: 1rpx solid #ddd;
  border-radius: 30rpx;
  padding: 10rpx 35rpx;
  font-size: 28rpx;
  margin: 10rpx;
}
.isbgc{
  color: #555 !important;
  background-color: #fff !important;
}
.btn{
  margin: 50rpx 0rpx;
}