/* pages/myself/myCollection/myCollection.wxss */
.container{
  box-sizing: border-box;
}
.header{
  width: 100%;
  display: flex;
  justify-content: center;
}
.header_title{
padding:  20rpx;
color: #555;
}
.header_title>text{
  display: inline-block;
padding-bottom: 10rpx;
}
.isactive{
  font-weight: bold;
}
.spanactive{
  width: 50rpx;
  height: 10rpx;
  margin: 0 auto;
  background-color: #333;
}
.details{
  width: 100%;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  box-sizing: border-box;
}
.all-scroll{
  width: 100%;
  height: 100%;
  white-space: nowrap;
}
.all-container{
  width: 100vw;
  display: inline-block;
  text-align: center;
  background-color: #fff;
  box-sizing: border-box;
}
